<h2>Pr√©-Agendamento</h2>

<form>
    <div>
        <label for="nome">Nome:</label>
        <input type="text" id="nome" placeholder="Nome">
    </div>
    <div>
        <label for="tel">Telefone:</label>
        <input type="tel" id="" placeholder="Telefone">
    </div>
    <div>
        <label for="data">Data:</label>
        <input type="date" id="data">
    </div>
    <div>
        <label for="animais">Selecione o animal:</label>
        <input list="animais" />
        <datalist id="animais">
            <option>Cachorro</option>
            <option>Gato</option>
            <option>Galinha</option>
            <option>Porco</option>
            <option>Cavalo</option>
            <option>Coelho</option>
        </datalist>
    </div>
    <div>
        <label for="need">Necessidade:</label>
        <textarea id="need" name="need" cols="30" rows="10"></textarea>
    </div>
    <input type="submit" value="ENVIAR" />
</form>